<%= simple_form_for(@treehouse) do |f| %>
  <%= f.input :name %>
  <%= f.input :tree_type %>
  <%= f.input :summary %>
  <%= f.input :address %>
  <%= f.input :total_occupancy %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit %>
<% end %>
